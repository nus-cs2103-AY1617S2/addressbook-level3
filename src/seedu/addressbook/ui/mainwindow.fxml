<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" stylesheets="@/seedu/addressbook/ui/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.addressbook.ui.MainWindow">

    <children>
      <SplitPane dividerPositions="0.8432835820895522">
         <items>
      
              <TextField fx:id="commandInput" onAction="#onCommand">
        </TextField>
            <ButtonBar>
              <buttons>
                <Button mnemonicParsing="false" onAction="#onListClick" onMouseClicked="#onListClick" text="List" />
                  <Button mnemonicParsing="false" onAction="#onClearClick" onMouseClicked="#onClearClick" text="Clear" />
              </buttons>
            </ButtonBar>
         </items>
      </SplitPane>

                    <TextArea fx:id="outputConsole" editable="false" wrapText="true" VBox.vgrow="ALWAYS">
        </TextArea>

    </children>

</VBox>
