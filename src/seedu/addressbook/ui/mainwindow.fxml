<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="center" stylesheets="@/seedu/addressbook/ui/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.addressbook.ui.MainWindow">

    <children>

        <TextField fx:id="commandInput" onAction="#onCommand" VBox.vgrow="NEVER">
         <font>
            <Font name="Book Antiqua" size="12.0" />
         </font></TextField>

        <TextArea fx:id="outputConsole" editable="false" wrapText="true" VBox.vgrow="ALWAYS">
         <font>
            <Font name="Book Antiqua" size="12.0" />
         </font></TextArea>
      <Label prefHeight="21.0" prefWidth="1200.0" text="Good Day! ^^">
         <font>
            <Font name="Book Antiqua" size="16.0" />
         </font>
      </Label>

    </children>

</VBox>
