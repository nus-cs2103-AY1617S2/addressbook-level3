<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" stylesheets="@/seedu/addressbook/ui/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.addressbook.ui.MainWindow">

    <children>

        <TextArea fx:id="outputConsole" editable="false" stylesheets="@DarkTheme.css" wrapText="true" VBox.vgrow="ALWAYS">
         <tooltip>
            <Tooltip text="TextArea" />
         </tooltip></TextArea>

        <TextField fx:id="commandInput" alignment="TOP_CENTER" onAction="#onCommand" stylesheets="@DarkTheme.css" VBox.vgrow="NEVER">
         <tooltip>
            <Tooltip text="Command Line" />
         </tooltip></TextField>

    </children>

</VBox>
