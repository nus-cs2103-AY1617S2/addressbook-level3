<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" stylesheets="@/seedu/addressbook/ui/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.addressbook.ui.MainWindow">

    <children>

        <TextField fx:id="commandInput" onAction="#onCommand" styleClass="text-field" text="Welcome to Address Book V3.0! " VBox.vgrow="ALWAYS">
        </TextField>

              <TextArea fx:id="outputConsole" editable="false" prefWidth="600.0" styleClass="text-area" stylesheets="@DarkTheme.css" wrapText="true" VBox.vgrow="ALWAYS">
        </TextArea>

    </children>

</VBox>
